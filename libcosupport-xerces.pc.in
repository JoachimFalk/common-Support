Requires.private: libcosupport-initializer = @PACKAGE_VERSION@
Libs: -L${libdir} -lcosupport-xerces
Libs.private: @XERCES_LDFLAGS@ -lxerces-c @XERCES_AUXLIBS@ @BOOST_LDFLAGS@ -lboost_thread@BOOST_LIBMTPOSTFIX@
Cflags: -I${includedir} @BOOST_INCLUDE@ @XERCES_INCLUDE@
